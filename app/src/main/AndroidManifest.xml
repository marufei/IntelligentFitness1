<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.health.demo.intelligentfitness">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ForgetPwdActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".ForgetPwd2Activity" />
        <activity android:name=".EditNameActivity" />
        <activity android:name=".EditPwdActivity" />
        <activity android:name=".EditHeightActivity" />
        <activity android:name=".EditWeightActivity"></activity>
    </application>

</manifest>